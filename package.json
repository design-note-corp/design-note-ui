    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-webpack5": "^6.4.22",
    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@storybook/testing-library": "^0.0.11",
    "@types/node": "^17.0.25",
    "@types/react": "^18.0.6",
    "babel-loader": "^8.2.5",
    "chromatic": "^6.5.4",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "npm-run-all": "^4.1.5",
